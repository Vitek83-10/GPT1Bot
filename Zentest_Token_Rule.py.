import asyncio
from pyrogram import Client

# –ö–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏—è
api_id = 20234202
api_hash = "fc0e099e810cbea903512acef8563b36"
bot_token = "8085881327:AAHtgjesSjMbyektB5W2YX1SDQAGk_MMPfc"
chat_id = 1047298304

# –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –∫–ª–∏–µ–Ω—Ç–∞
app = Client("bot", api_id=api_id, api_hash=api_hash, bot_token=bot_token)

# –§—É–Ω–∫—Ü–∏—è –æ—Ç–ø—Ä–∞–≤–∫–∏
async def send_token():
    async with app:
        await app.send_message(chat_id, "üß™ Zentest Rule:\n`FAKE_XYZ_TOKEN_001_TEST`", parse_mode="Markdown")

# –ó–∞–ø—É—Å–∫
if __name__ == "__main__":
    asyncio.run(send_token())
